{
  //  Window
  "window.zoomLevel": 0,
  "window.commandCenter": false,
  "window.dialogStyle": "custom",
  "window.restoreWindows": "none",
  "window.titleBarStyle": "custom",
  "window.title": "${activeEditorLong}",
  "window.customTitleBarVisibility": "auto",
  //  Fonts, Theme and Icon
  "editor.fontLigatures": true,
  "editor.fontWeight": "normal",
  "workbench.iconTheme": "symbols",
  "terminal.integrated.fontSize": 12,
  "workbench.colorTheme": "o9 Theme",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.preferredDarkColorTheme": "o9 Theme",
  "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
  //  Suggestion
  "breadcrumbs.enabled": true,
  "explorer.compactFolders": true,
  "window.menuBarVisibility": "compact",
  "window.autoDetectColorScheme": false,
  "powershell.developer.setExecutionPolicy": true,
  //  Privacy and  Security
  "update.mode": "manual",
  "extensions.autoUpdate": false,
  "update.showReleaseNotes": false,
  "telemetry.telemetryLevel": "off",
  "npm.fetchOnlinePackageInfo": false,
  "extensions.verifySignature": false,
  "extensions.autoCheckUpdates": false,
  "security.restrictUNCAccess": false,
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.emptyWindow": true,
  "update.enableWindowsBackgroundUpdates": false,
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.startupPrompt": "never",
  "extensions.experimental.issueQuickAccess": false,
  // Workbench
  "workbench.tree.indent": 16,
  "workbench.tips.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": true,
  "workbench.enableExperiments": false,
  "workbench.sideBar.location": "right",
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.enablePreview": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.showTabs": "multiple",
  "workbench.list.openMode": "doubleClick",
  "workbench.editor.closeEmptyGroups": true,
  "workbench.activityBar.location": "default",
  "workbench.editor.closeOnFileDelete": false,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.tabActionLocation": "left",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.editor.tabSizingFixedMinWidth": 40,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.tabSizingFixedMaxWidth": 80,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.alwaysShowEditorActions": false,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  //  Debug
  "debug.console.fontSize": 15,
  "debug.inlineValues": "on",
  "debug.onTaskErrors": "abort",
  "debug.confirmOnExit": "never",
  "debug.console.lineHeight": 35,
  "debug.console.closeOnEnd": true,
  "debug.toolBarLocation": "docked",
  "debug.autoExpandLazyVariables": "on",
  "debug.allowBreakpointsEverywhere": true,
  // Other
  "scm.diffDecorations": "all",
  "zenMode.centerLayout": false,
  "search.showLineNumbers": true,
  "explorer.confirmDelete": false,
  "outline.problems.badges": false,
  "git.decorations.enabled": false,
  "explorer.openEditors.visible": 1,
  "search.decorations.badges": false,
  "diffEditor.renderSideBySide": true,
  "zenMode.silentNotifications": true,
  "emmet.triggerExpansionOnTab": true,
  "window.confirmBeforeClose": "never",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "explorer.decorations.badges": false,
  "problems.decorations.enabled": false,
  "accessibility.signalOptions.volume": 0,
  "window.newWindowDimensions": "inherit",
  "emmet.showSuggestionsAsSnippets": true,
  "extensions.ignoreRecommendations": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "window.density.editorTabHeight": "compact",
  "window.confirmSaveUntitledWorkspace": false,
  //  Editor
  "editor.tabSize": 2,
  "editor.lineHeight": 25,
  "editor.wordWrap": "on",
  "editor.folding": false,
  "editor.lineNumbers": "on",
  "editor.formatOnSave": true,
  "editor.letterSpacing": 0.3,
  "editor.glyphMargin": false,
  "editor.autoIndent": "full",
  "editor.hover.sticky": false,
  "editor.formatOnType": false,
  "editor.tabCompletion": "on",
  "editor.hover.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.matchBrackets": "near",
  "editor.formatOnPaste": false,
  "editor.smoothScrolling": true,
  "editor.minimap.enabled": true,
  "editor.minimap.autohide": true,
  "editor.cursorBlinking": "solid",
  "editor.renderWhitespace": "none",
  "editor.quickSuggestionsDelay": 0,
  "editor.suggest.showWords": false,
  "editor.suggest.showIssues": true,
  "editor.suggest.showClasses": true,
  "editor.suggest.showMethods": true,
  "editor.guides.indentation": false,
  "editor.suggest.localityBonus": true,
  "editor.suggest.showFunctions": true,
  "editor.suggest.showVariables": true,
  "editor.suggest.showConstants": true,
  "editor.accessibilitySupport": "off",
  "editor.occurrencesHighlight": "off",
  "editor.stickyScroll.enabled": false,
  "editor.suggest.filterGraceful": true,
  "editor.suggest.insertMode": "insert",
  "editor.scrollbar.vertical": "hidden",
  "editor.minimap.showSlider": "always",
  "editor.renderLineHighlight": "gutter",
  "editor.guides.bracketPairs": "active",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderControlCharacters": false,
  "editor.minimap.renderCharacters": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.guides.highlightActiveIndentation": true,
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.quickSuggestions": {
    "other": true,
    "strings": true,
    "comments": true
  },
  //  Terminal
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.splitCwd": "inherited",
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.confirmOnKill": "never",
  "terminal.integrated.gpuAcceleration": "auto",
  "terminal.integrated.tabs.defaultIcon": "github",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.external.windowsExec": "C:\\Users\\o9\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell",
      "args": ["-NoLogo"]
    },
    "Command Prompt": {
      "path": ["${env:windir}\\Sysnative\\cmd.exe", "${env:windir}\\System32\\cmd.exe"],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    }
  },
  //  POWERSHELL
  "powershell.pester.codeLens": true,
  "powershell.enableProfileLoading": true,
  "powershell.promptToUpdatePowerShell": true,
  "powershell.pester.useLegacyCodeLens": false,
  "powershell.codeFormatting.preset": "Custom",
  "powershell.codeFormatting.useCorrectCasing": true,
  "powershell.integratedConsole.showOnStartup": false,
  "powershell.codeFormatting.autoCorrectAliases": true,
  "powershell.codeFormatting.ignoreOneLineBlock": true,
  "powershell.codeFormatting.whitespaceInsideBrace": true,
  "powershell.codeFormatting.newLineAfterCloseBrace": true,
  "powershell.codeFormatting.addWhitespaceAroundPipe": true,
  "powershell.codeFormatting.alignPropertyValuePairs": true,
  "powershell.integratedConsole.suppressStartupBanner": true,
  "powershell.codeFormatting.whitespaceAfterSeparator": true,
  "powershell.codeFormatting.whitespaceAroundOperator": true,
  "powershell.codeFormatting.whitespaceBeforeOpenBrace": true,
  "powershell.codeFormatting.whitespaceBeforeOpenParen": true,
  "powershell.integratedConsole.focusConsoleOnExecute": false,
  "powershell.codeFormatting.trimWhitespaceAroundPipe": false,
  "powershell.debugging.createTemporaryIntegratedConsole": true,
  "powershell.codeFormatting.whitespaceBetweenParameters": true,
  "powershell.powerShellDefaultVersion": "Windows PowerShell (x64)",
  "powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationAfterEveryPipeline",
  //  Git
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.inputValidation": false,
  "git.enableSmartCommit": true,
  "git.postCommitCommand": "sync",
  "git.showPushSuccessNotification": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  //  GITHUB COPILOT
  "editor.suggestSelection": "first",
  "editor.inlineSuggest.enabled": true,
  "editor.snippetSuggestions": "inline",
  "editor.parameterHints.enabled": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.inlineSuggest.showToolbar": "onHover",
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.chat.agent.thinkingTool": true,
  "github.copilot.editor.enableCodeActions": true,
  "editor.inlineSuggest.suppressSuggestions": false,
  "github.copilot.chat.generateTests.codeLens": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "editor.wordBasedSuggestions": "matchingDocuments",
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": true
  },
  "github.copilot.advanced": {
    "enable": {
      "*": true,
      "agentMode": "edit",
      "model": "claude-sonnet-3.7"
    },
    "inlineSuggest.enable": true,
    "listCount": 5,
    "debug.showScores": false
  },
  //  Prettier Extension
  "prettier.semi": true,
  "prettier.tabWidth": 2,
  "prettier.endOfLine": "lf",
  "prettier.printWidth": 100,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.bracketSpacing": true,
  "prettier.jsxSingleQuote": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  //  ESLint Extension
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html"
  ],
  //  Path Intellisense Extensio
  "path-intellisense.mappings": {
    "/": "${workspaceFolder}",
    "lib": "${workspaceFolder}/lib",
    "src": "${workspaceFolder}/src"
  },
  //  Markdown All in One Extensio
  "markdown.extension.toc.levels": "2..6",
  "markdown.extension.toc.slugifyMode": "github",
  "markdown.extension.preview.autoShowPreviewToSide": false,
  //  Better Comments Extensio
  "better-comments.multilineComments": true,
  "better-comments.highlightPlainText": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "backgroundColor": "transparent"
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "backgroundColor": "transparent"
    }
  ],
  "files.associations": {
    ".env*": "properties",
    "*.ps1": "powershell",
    "*.json": "jsonc"
  },
  //  Files
  "files.eol": "\n",
  "files.autoSave": "off",
  "files.encoding": "utf8",
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/dist/**": true,
    "**/build/**": true,
    "**/.hg/store/**": true,
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/.git/subtree-cache/**": true
  },
  "search.exclude": {
    "**/dist": true,
    "**/coverage": true,
    "**/node_modules": true,
    "**/bower_components": true
  },
  "files.exclude": {
    "**/.hg": true,
    "**/CVS": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.project": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.settings": true,
    "**/.classpath": true,
    "**/.factorypath": true
  },
  //  JavaScript & TypeScript
  "typescript.format.enable": true,
  "javascript.format.enable": true,
  "typescript.suggest.autoImports": true,
  "javascript.suggest.autoImports": true,
  "typescript.preferences.quoteStyle": "single",
  "javascript.referencesCodeLens.enabled": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.importModuleSpecifier": "shortest",
  //  HTML
  "html.format.wrapLineLength": 120,
  "html.format.wrapAttributes": "auto",
  "html.completion.attributeDefaultValue": "doublequotes",
  //  CSS, SCSS, LESS
  "css.validate": true,
  "less.validate": true,
  "scss.validate": true,
  "css.lint.unknownAtRules": "ignore",
  //  PHP
  "php.suggest.basic": true,
  "php.validate.enable": true,
  //  JSON & YAML
  "json.format.enable": true,
  "security.promptForRemoteFileProtocolHandling": false,
  "security.promptForLocalFileProtocolHandling": false,
  "editor.overtypeCursorStyle": "line"
}
